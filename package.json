{
  "name": "my-store-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ignore-path .gitignore --fix src",
    "prettier": "prettier src --write",
    "cdk": "npm run build && cdk",
    "nested:install":"npm install  --prefix ./cdk/",
    "nested:build":"npm run build  --prefix cdk",
    "nested:deploy":"npm run deploy  --prefix cdk",
    "deploy": "npm run build && npm run nested:install && npm run nested:build && npm run nested:deploy"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.341.0",
    "@aws-sdk/credential-provider-ini": "^3.341.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.7",
    "@mui/styles": "^5.8.7",
    "@types/yup": "^0.29.14",
    "aws-cdk-lib": "^2.81.0",
    "aws-sdk": "^2.1386.0",
    "axios": "^0.27.2",
    "constructs": "^10.2.37",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0",
    "uuid": "^9.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.18.0",
    "c8": "^7.11.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "jsdom": "^20.0.0",
    "msw": "^0.43.1",
    "prettier": "2.7.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vitest": "^0.17.1"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
